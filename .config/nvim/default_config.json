{
  "plugins": {
    "aiAssistant": {
      "githubCopilot": {
        "enabled": false,
        "confirm_tab": false
      },
      "avante": {
        "enabled": false,
        "opts": {
          "provider": "copilot",
          "providers": {
            "ollama": {
              "endpoint": "127.0.0.1:11434",
              "model": "qwen2.5-coder:1.5b-base"
            },
            "copilot": {
              "model": "gpt-4o"
            }
          }
        }
      }
    },
    "animation": {
      "enabled": false
    },
    "complement": {
      "enabled": true,
      "engine": "blink-cmp"
    },
    "file_explorer": {
      "name": "yazi"
    },
    "fuzzy_finder": {
      "telescope": {
        "find_command": [
          "rg",
          "--files",
          "--hidden",
          "--no-ignore",
          "--glob",
          "!.git/*",
          "--glob",
          "!*.cache/*",
          "--glob",
          "!.venv/*",
          "--glob",
          "!*.pyc"
        ]
      }
    },
    "inputBehavior": {
      "hardtime": {
        "enabled": false
      }
    },
    "rest": {
      "enabled": false
    },
    "rsync": {
      "transfer": {
        "upload_rsync_params": [
          "-rlzi",
          "--checksum",
          "--exclude",
          ".git",
          "--exclude",
          ".idea",
          "--exclude",
          ".DS_Store",
          "--exclude",
          ".nvim",
          "--exclude",
          "*.pyc"
        ],
        "download_rsync_params": [
          "-rlzi",
          "--checksum",
          "--exclude",
          ".git"
        ]
      }
    }
  },
  "format": {
    "format_on_save": true
  },
  "colorscheme": {
    "name": "vim",
    "opt": {}
  }
}
